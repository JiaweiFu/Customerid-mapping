<types xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	   xmlns="http://www.hybris.com/schema/ondemand/domain"
	   xsi:schemaLocation="http://www.hybris.com/schema/ondemand/domain http://www.hybris.com/schema/ondemand/domain-1.6.xsd"
	   package="com.hybris.datahub.customeridmapping.rest.resources">


	<managedobjects>
		<managedobject code="Cow">
			<description>Structure to hold Cow.</description>
			<persistence name="Cow" />
			<attributes>
				<attribute name="feedId" type="long">
					<description>The primary key id of this artifact</description>
					<modifiers mandatory="true" generated="true" final="true" />
				</attribute>
				<attribute name="name" type="String">
					<description>The name of this data feed</description>
					<modifiers mandatory="true" unique="false" />
				</attribute>
			</attributes>
			<pk>
				<attribute name="feedId" />
			</pk>
		</managedobject>		
	</managedobjects>
</types>
